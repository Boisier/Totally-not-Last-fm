    }],
    "xAxes": [{
      "display": false,
      "gridLines": {
        "display": true,
        "drawOnChartArea": true,
        "drawTicks": false
      },
      "ticks": {
        "display": false,
        "fontSize": 15,
        "fontColor": "#1e1e1e",
        "fontStyle": "bold",
        "fontFamily": "Arial"
      }
    }]
  },
  "elements": {
    "point": {
      "radius": 0,
      "hitRadius": 0,
      "hoverRadius": 0,
      "borderWidth": 0,
      "borderColor": "rgba(255, 0, 0, 0)"
    }
  }
