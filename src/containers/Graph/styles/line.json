{
  "tooltips": {
    "enabled": false,
    "mode": "index",
    "intersect": false,
    "custom": null
  },
  "title": {
    "display": false,
    "text": "Plays per day",
    "fontSize": 20
  },
  "legend": {
    "display": false,
    "labels": {
      "hidden": true
    }
  },
  "scales": {
    "yAxes": [{
      "display": false,
      "gridLines": {
        "display": true,
        "drawTicks": false
      },
      "ticks": {
